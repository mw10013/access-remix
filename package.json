{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "pnpm generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "pnpm generate:css --watch",
    "dev:remix": "remix dev",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "test": "vitest",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc -b",
    "db:reset": "supabase db reset",
    "typegen": "supabase gen types typescript --local > db_types.ts"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@headlessui/react": "1.7.4",
    "@heroicons/react": "2.0.13",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@remix-run/vercel": "^1.7.6",
    "@supabase/auth-helpers-remix": "0.1.3",
    "@supabase/supabase-js": "2.1.3",
    "@vercel/node": "^2.6.4",
    "clsx": "1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remeda": "1.3.0",
    "tiny-invariant": "1.3.1",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.0",
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.8",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "8.5.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "supabase": "1.22.3",
    "tailwindcss": "3.2.4",
    "typescript": "^4.9.3",
    "vitest": "^0.26.0"
  }
}
